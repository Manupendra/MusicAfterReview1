<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.music">

    <application
        android:allowBackup="true"
        android:icon="@drawable/music"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/home" />
        <activity
            android:name=".NowPlayingActivity"
            android:label="@string/nowPlaying" />
        <activity
            android:name=".ChartsActivity"
            android:label="@string/charts" />
        <activity
            android:name=".NewReleaseActivity"
            android:label="@string/newRelease" />
        <activity
            android:name=".LibraryActivity"
            android:label="@string/library" />
        <activity
            android:name=".SearchActivity"
            android:label="@string/search" />
        <activity
            android:name=".SettingsActivity"
            android:label="@string/setting" />
        <activity
            android:name=".BuyActivity"
            android:label="Payment"/>
        <activity
            android:name=".DetailsOfSongActivity"
            android:label="Details of Song">
        </activity>
    </application>

</manifest>